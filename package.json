{
  "name": "you-engine",
  "version": "0.1.3",
  "description": "A lightweight, pluggable 2D Canvas game framework",
  "author": "atshelchin",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/atshelchin/you-engine.git"
  },
  "homepage": "https://github.com/atshelchin/you-engine#readme",
  "bugs": {
    "url": "https://github.com/atshelchin/you-engine/issues"
  },
  "type": "module",
  "packageManager": "pnpm@9.14.2",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=9.0.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./systems": {
      "import": "./dist/systems/index.js",
      "types": "./dist/systems/index.d.ts"
    },
    "./core": {
      "import": "./dist/core/index.js",
      "types": "./dist/core/index.d.ts"
    },
    "./math": {
      "import": "./dist/math/index.js",
      "types": "./dist/math/index.d.ts"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prepublishOnly": "pnpm build",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "lint": "biome lint --write .",
    "lint:check": "biome lint .",
    "check": "biome check --write .",
    "check:ci": "biome check ."
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "@tweenjs/tween.js": "^23.1.3",
    "howler": "^2.2.4",
    "matter-js": "^0.20.0",
    "miniplex": "^2.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.8",
    "@types/howler": "^2.2.12",
    "@types/matter-js": "^0.19.7",
    "@types/stats.js": "^0.17.3",
    "stats.js": "^0.17.0",
    "typescript": "^5.3.3",
    "vite": "^5.4.11",
    "vite-plugin-dts": "^4.3.0"
  },
  "keywords": [
    "game",
    "engine",
    "canvas",
    "2d",
    "typescript",
    "ecs"
  ],
  "license": "MIT"
}
